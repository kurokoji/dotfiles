{
	"languageserver": {
		"ccls": {
			"command": "ccls",
			"filetypes": [
				"c",
				"cpp",
				"objc",
				"objcpp"
			],
			"rootPatterns": [
				".ccls",
				"compile_commands.json",
				".vim/",
				".git/",
				".hg/"
			],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				},
				"clang": {
					"resourceDir": "/Library/Developer/CommandLineTools/usr/lib/clang/11.0.0",
					"extraArgs": [
						"-isystem",
						"/Library/Developer/CommandLineTools/usr/include/c++/v1",
						"-I",
						"/Library/Developer/CommandLineTools/usr/include/"
					]
				}
			}
		},
		"dls": {
			"command": "dls",
			"args": [
				"--stdio"
			],
			"filetypes": [
				"d"
			],
			"trace.server": "off",
			"rootPatterns": [
				"dub.json",
				"dub.sdl"
			],
			"initializationOptions": {},
			"settings": {
				"d": {
					"dls": {
						"format": {
							"dfmtBraceStyle": "otbs",
							"dfmtSpaceAfterCast": false
						},
						"symbol": {
							"importPaths": [
								"/Users/nazuna/.dub/packages/nephele-0.0.3/nephele/source"
							],
							"autoImports": true
						}
					}
				}
			}
		},
		"bash": {
			"command": "bash-language-server",
			"args": [
				"start"
			],
			"filetypes": [
				"sh"
			],
			"ignoredRootPaths": [
				"~"
			]
		}
	}
}
